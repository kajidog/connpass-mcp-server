    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/qs": "^6.9.10",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "connpass",
    "api",
    "typescript",
    "client"
  ],
  "type": "commonjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kajidog/connpass-in-chatgpt.git",
    "directory": "packages/api-client"
  },
  "bugs": {
    "url": "https://github.com/kajidog/connpass-in-chatgpt/issues"
  },
  "homepage": "https://github.com/kajidog/connpass-in-chatgpt#readme",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md"
  ],
