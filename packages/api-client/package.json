{
  "name": "@kajidog/connpass-api-client",
  "version": "0.1.0",
  "description": "TypeScript client for Connpass API v2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
